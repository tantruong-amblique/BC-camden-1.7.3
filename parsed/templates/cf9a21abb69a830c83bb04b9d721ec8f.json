{
  "components/common/store-logo": "<a href=\"{{urls.home}}\" {{#if theme_settings.enable_sticky_navigation}}data-topbar data-options=\"sticky_on: large\"{{/if}}>\n  {{#if theme_settings.svg_logo}}\n      <span class=\"header-logo-text with-image\">\n          <img class=\"header-logo-image svg-logo\" data-sizes=\"auto\" src=\"{{cdn 'webdav:img/'}}{{theme_settings.svg_logo}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\" width=\"{{first (split theme_settings.logo_size 'x')}}\" height=\"{{last (split theme_settings.logo_size 'x')}}\">\n      </span>\n  {{else}}\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <span class=\"header-logo-text\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </span>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n  {{/if}}\n</a>\n"
}