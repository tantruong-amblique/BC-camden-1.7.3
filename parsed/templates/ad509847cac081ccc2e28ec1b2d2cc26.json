{
  "components/f/header/top-utilities": "<nav class=\"navUser sticky\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"><span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-gift\"></use></svg></span><span class=\"navUser-text\"> {{lang 'common.gift_cert'}}</span></a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            <a class=\"navUser-action {{#if customer}}has-dropdown{{/if}}\" href=\"{{urls.account.index}}\" {{#if customer}}data-dropdown=\"accountOptions\" aria-controls=\"accountOptions\" aria-expanded=\"false\" data-account-menu{{/if}}><span class=\"top-utilities-icon\"><svg><use xlink:href=\"#icon-user\"></use></svg></span><span class=\"navUser-text\"> {{lang 'common.account'}}</span> {{#if customer}}<span class=\"navUser-dropdown-arrow\"><span class=\"top-utilities-icon top-utilities-icon--arrow\"><svg><use xlink:href=\"#icon-angle-down\"></use></svg></span></span>{{/if}}</a>\n            {{#if customer}}\n            <ul class=\"dropdown-menu dropdown-menu--arrow-right is-open f-open-dropdown\" id=\"accountOptions\" data-dropdown-content=\"\" aria-hidden=\"false\" tabindex=\"-1\">\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                </li>\n                {{#if settings.returns_enabled}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.inbox}}\">{{lang 'header.messages'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n                </li>\n                {{#if settings.show_payment_methods}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                </li>\n                {{/if}}\n                {{#if settings.show_wishlist}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.wishlists.all}}\">{{lang 'header.wishlists.no_name'}}</a>\n                </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                </li>\n            </ul>\n            {{/if}}\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item store-credit\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\">{{lang 'header.store_credit'}}<span class=\"store-credit__amount\"> {{customer.store_credit.formatted}}</span></a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}