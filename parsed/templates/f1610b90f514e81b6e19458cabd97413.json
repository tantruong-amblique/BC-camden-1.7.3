{
  "components/amp/common/store-logo": "<a href=\"{{urls.home}}\" class=\"amp-header-link\">\n    {{#if settings.store_logo.image}}\n        <amp-img src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n            {{#if theme_settings.logo_size '===' 'original' }}\n                width=\"200\"\n                height=\"40\"\n                layout=\"responsive\"\n                sizes=\"200px\"\n            {{else}}\n                width=\"{{#if (first (split theme_settings.logo_size 'x')) '<' 200}}{{first (split theme_settings.logo_size 'x')}}px{{else}}200px{{/if}}\"\n                height=\"{{#if (last (split theme_settings.logo_size 'x')) '<' 40}}{{last (split theme_settings.logo_size 'x')}}px{{else}}40px{{/if}}\"\n                layout=\"responsive\"\n                sizes=\"{{#if (first (split theme_settings.logo_size 'x')) '<' 200}}{{first (split theme_settings.logo_size 'x')}}px{{else}}200px{{/if}}\"\n            {{/if}}\n            alt=\"{{settings.store_logo.title}}\">\n        </amp-img>\n    {{else}}\n        <span class=\"amp-header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"
}