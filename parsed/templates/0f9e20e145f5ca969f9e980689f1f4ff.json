{
  "components/f/b2b/quick-add-form": "<div class=\"sku-add sku-add--sku\">\n    <form class=\"form js-quick-add\" action=\"{{urls.cart}}\" method=\"post\" enctype=\"multipart/form-data\" data-error=\"{{lang 'b2b.side_cart.popup_messages.sku_error'}}\">\n    <div class=\"add-sku-item\"></div>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <div class=\"form-input-wrap\">\n                    <input class=\"form-input form-input--sku-num\" name=\"sku\" type=\"text\" placeholder=\"{{lang 'b2b.header.sku_text'}}\" data-sku autocorrect=\"off\">\n                    <input class=\"form-input form-input--sku-qty\" name=\"qty\" type=\"tel\" value=\"1\" min=\"1\" data-qty autocomplete=\"off\">\n                </div>\n                <span class=\"quick-add-result\"></span>\n                <button type=\"submit\" class=\"search-button\" type=\"submit\">{{lang 'b2b.header.sku_button'}}</button>\n            </div>\n        </fieldset>\n    </form>\n</div>\n"
